<Server port="8015" shutdown="SHUTDOWN">
    <!--APR library loader. Documentation at /docs/apr.html -->
    <Listener className="org.apache.catalina.core.AprLifecycleListener" SSLEngine="on" />
    <Service name="Catalina">
        <Connector port="8081" URIEncoding="UTF-8" protocol="HTTP/1.1"/>

        <!-- This is the optional AJP connector -->
        <Connector port="8019" URIEncoding="UTF-8" protocol="AJP/1.3"  redirectPort="8443" />
        <!--START :: SALT :: configure_ssl_connector DO NOT EDIT MANUALLY -->
        
        <!--END :: SALT :: configure_ssl_connector DO NOT EDIT MANUALLY -->    
        <Engine name="Catalina" defaultHost="localhost">
            <Host name="localhost" appBase="webapps"/>
        </Engine>

    </Service>
</Server>